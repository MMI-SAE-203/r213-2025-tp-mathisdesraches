---
import Layout from "../../layouts/Base.astro";
import { getAgents } from "../../backend.mjs";

const agents = await getAgents();
console.log(agents);

---

<Layout title="Agents">
    <h1 class="text-6xl">Agents</h1>
    <ul class="mt-4">
        {
            agents?.map((agent) => (
                <li>
                    <a href={`/agents/${agent.id}`}>
                        {agent.Nom}
                    </a>
                </li>
            ))
        }
    </ul>
</Layout>